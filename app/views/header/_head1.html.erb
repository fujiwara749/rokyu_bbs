<% if user_signed_in? %>
      <!--エンドユーザーログイン済み-->
      <ul class="nav navbar-nav navbar-right">
        
        <li><%= link_to 'アカウント情報', user_path(current_user.id), class:"header-link" %></li>
        <li><%= link_to 'ログアウト', destroy_user_session_path, method: :delete, class:"header-link" %></li>
      </ul>
    <% else %>
      <!--エンドユーザーログアウト状態-->
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to '新規登録', new_user_registration_path, class:"header-link" %></li>
        <li><%= link_to 'ログイン', user_session_path, class:"header-link" %></li>
      </ul>
    <% end %>